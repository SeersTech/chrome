{
  "manifest_version": 2,

  "name": "TheBrain",
  "description": "Collects web activity and sends to TheBrain",
  "version": "0.1",
  "background": {
	"scripts": ["background.js"]
  },
  "permissions": [
	"history",
	"http://*/",
	"tabs",
	"storage"
	//"fileSystem"
   ],
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}